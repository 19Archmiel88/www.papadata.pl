{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "transformIgnorePatterns": [],
  "globals": {
    "ts-jest": {
      "tsconfig": {
        "allowJs": true
      }
    }
  },
  "collectCoverageFrom": ["src/**/*.(t|j)s"],
  "testEnvironment": "node"
}
